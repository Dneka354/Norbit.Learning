using System;

class Program
{
    static void Main()
    {
        string s = Console.ReadLine();
        string target = "hello";
        int targetIndex = 0;

        for (int i = 0; i < s.Length; i++)
        {
            if (s[i] == target[targetIndex])
            {
                targetIndex++;
                if (targetIndex == target.Length)
                {
                    break;
                }
            }
        }

        Console.WriteLine(targetIndex == target.Length ? "YES" : "NO");
    }
}
